{
  "compilerOptions": {
    "baseUrl": "./resources/js",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@Components/*": [
        "./components/*"
      ],
      "@Config/*": [
        "./config/*"
      ],
      "@Css/*": [
        "./css/*"
      ],
      "@Hooks/*": [
        "./hooks/*"
      ],
      "@Images/*": [
        "./images/*"
      ],
      "@Services/*": [
        "./services/*"
      ],
      "@Util/*": [
        "./util/*"
      ]
    }
  },
  "include": [
    "resources/js"
  ]
}